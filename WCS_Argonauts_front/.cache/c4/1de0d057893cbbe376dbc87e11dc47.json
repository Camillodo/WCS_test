t';\n  const [argonauts, setArgonauts] = useState([]);\n  const [nameInput, setNameInput] = useState('');\n\n  const loadArgonauts = () => {\n    axios.get(APIURL)\n      .then((res) => {\n        setArgonauts(res.data);\n      })\n      .catch((err) => {\n        console.error(err);\n      });\n  };\n\n  const handleNameInput = (event) => {\n    setNameInput(event.target.value);\n  };\n\n  const addArgonaut = () => {\n    axios.post(APIURL, { name: nameInput })\n      .then(() => {\n        loadArgonauts();\n      })\n      .catch((err) => {\n        console.error(err);\n      });\n  };\n\n  const handleSubmit = (event) => {\n    event.preventDefault();\n    addArgonaut();\n  };\n\n  useEffect(() => {\n    loadArgonauts();\n  }, []);\n\n  return (\n    <div className=\"app\">\n      <Header />\n      <main>\n        <NewMemberForm handleNameInput={handleNameInput} handleSubmit={handleSubmit} />\n        <MemberList argonauts={argonauts} />\n      </main>\n      <Footer />\n    </div>\n  );\n};\n// == Export\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"05b5c9f96f05655a75c29f8fc9257124","cacheData":{"env":{}}}